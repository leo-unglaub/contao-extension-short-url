-- 
-- Table `tl_short_url_cache`
-- 

CREATE TABLE `tl_short_url_cache` (
  `id` int(10) unsigned NOT NULL auto_increment,
  `tstamp` int(10) NOT NULL default '0',
  `expires` int(10) NOT NULL default '0',
  `shortUrl` text NULL,
  `longUrl` text NULL,
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;
